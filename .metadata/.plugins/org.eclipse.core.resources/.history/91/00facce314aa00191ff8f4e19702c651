import com.bridgelab.beans.BeanTest;

public class Client 
{
public static void main(String arg[])
{
	ApplicationContext context=new ClassPathXmlApplicationContext("/home/admin1/Documents/SpringCore/com.bridgelab.spring/src/main/java/Spring/com/bridgelab/spring/Spring.xml");
	BeanTest bean=(BeanTest) context.getBean("t");
	System.out.println(bean);
}
}
